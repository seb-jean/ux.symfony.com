<twig:Item:Group class="max-w-sm">
    <twig:Item>
        <twig:Item:Media variant="icon">
            <twig:ux:icon name="lucide:inbox" class="size-4" />
        </twig:Item:Media>
        <twig:Item:Content>
            <twig:Item:Title>Inbox</twig:Item:Title>
            <twig:Item:Description>View all your messages</twig:Item:Description>
        </twig:Item:Content>
    </twig:Item>
    <twig:Item:Separator />
    <twig:Item>
        <twig:Item:Media variant="icon">
            <twig:ux:icon name="lucide:send" class="size-4" />
        </twig:Item:Media>
        <twig:Item:Content>
            <twig:Item:Title>Sent</twig:Item:Title>
            <twig:Item:Description>Messages you have sent</twig:Item:Description>
        </twig:Item:Content>
    </twig:Item>
    <twig:Item:Separator />
    <twig:Item>
        <twig:Item:Media variant="icon">
            <twig:ux:icon name="lucide:archive" class="size-4" />
        </twig:Item:Media>
        <twig:Item:Content>
            <twig:Item:Title>Archive</twig:Item:Title>
            <twig:Item:Description>Archived messages</twig:Item:Description>
        </twig:Item:Content>
    </twig:Item>
</twig:Item:Group>
